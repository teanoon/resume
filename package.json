{
  "name": "resume",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.16.1",
    "date-fns": "^3.6.0",
    "i18next": "^23.11.1",
    "i18next-http-backend": "^2.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-router": "^6.22.3",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "react-to-print": "^2.15.1",
    "typescript": "^5.4.5",
    "web-vitals": "^3.5.2"
  },
  "overrides": {
    "typescript": "^5.4.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && rm -rf docs && mv build docs",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "i18n:zh-CN": "i18next src/**/*.{ts,tsx} -o public/locales/zh-CN/translation.json",
    "i18n:en-US": "i18next src/**/*.{ts,tsx} -o public/locales/en-US/translation.json",
    "i18n": "npm run i18n:zh-CN && npm run i18n:en-US"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.2.77",
    "@types/react-dom": "^18.2.0",
    "@types/react-router-dom": "^5.3.3",
    "i18next-parser": "^8.13.0",
    "node-sass": "^9.0.0"
  }
}
