{
  "name": "resume",
  "homepage": "https://teanoon.github.io/resume",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.1",
    "antd": "^4.13.1",
    "date-fns": "^2.19.0",
    "i18next": "^19.9.1",
    "i18next-http-backend": "^1.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.8",
    "react-scripts": "4.0.3",
    "react-to-print": "^2.12.3",
    "typescript": "^4.2.3",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "i18n:zh-CN": "i18next src/**/*.{ts,tsx} -o public/locales/zh-CN/translation.json",
    "i18n:en-US": "i18next src/**/*.{ts,tsx} -o public/locales/en-US/translation.json",
    "i18n": "npm run i18n:zh-CN && npm run i18n:en-US"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "i18next-parser": "^3.7.0",
    "node-sass": "^5.0.0"
  }
}
